<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>br.com.favarettodev</groupId>
	<artifactId>CalculadoraComJUnit</artifactId>
	<version>1.0</version>
	<name>CalculadoraComJUnit</name>
	<description>Este projeto é uma calculadora simples, que realiza operações de soma, subtração, divisão e multiplicação. Este projeto está sendo desenvolvido com a intensão de aumentar meus conhecimentos com testes unitários através do jUnit.	</description>

	<properties>
		<maven.compiler.source>1.7</maven.compiler.source>
		<maven.compiler.target>1.7</maven.compiler.target>
	</properties>

	<dependencies>
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<version>4.13.2</version>
		</dependency>
	</dependencies>
</project>